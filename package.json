{
  "name": "google-places-scrapper",
  "productName": "google-places-scrapper",
  "version": "1.0.0",
  "description": "Application to export places data from google API.",
  "main": "src/index.js",
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint src --color"
  },
  "keywords": [],
  "author": "Przemys≈Çaw Wrzeszcz",
  "license": "BOX",
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "dmg",
          "zip"
        ],
        "linux": [
          "deb"
        ]
      },
      "electronPackagerConfig": {
        "ignore": ["\\.idea"],
        "packageManager": "npm"
      },
      "electronWinstallerConfig": {
        "name": "google_places_scrapper",
        "setupIcon": "./src/markup/Install.ico"
      },
      "electronInstallerDebian": {
        "icon": "./src/markup/icon.png",
        "categories": [
          "Utility"
        ]
      },
      "electronInstallerDMG": {
        "background": "./src/markup/background-mac-installer.png",
        "icon": "./src/markup/icon.icns",
        "format": "ULFO",
        "overwrite": true
      },
      "github_repository": {
        "owner": "",
        "name": ""
      },
      "windowsStoreConfig": {
        "packageName": "",
        "name": "googleplacesscrapper"
      }
    }
  },
  "dependencies": {
    "@google/maps": "^0.5.5",
    "electron-compile": "^6.4.3",
    "electron-squirrel-startup": "^1.0.0",
    "excel4node": "^1.6.0",
    "jquery": "^3.3.1",
    "locutus": "^2.0.15"
  },
  "devDependencies": {
    "babel-plugin-transform-async-to-generator": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "electron-forge": "^5.2.2",
    "electron-prebuilt-compile": "2.0.7",
    "eslint": "^3.19.0",
    "eslint-config-airbnb": "^15.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^5.1.1",
    "eslint-plugin-react": "^7.11.1"
  }
}
